{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kafka": {
    "ProducerConfig": {
      "BootstrapServers": "kafka:9092",
      "ClientId": "hmem-producer",
      "Acks": "All",
      "MessageSendMaxRetries": 3,
      "RetryBackoffMs": 1000
    }
  }
}
